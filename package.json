{
  "name": "strapi-plugin-icon-picker",
  "version": "0.1.0",
  "description": "Icon picker custom field for Strapi v5 (auto-discovers React/SVG icons).",
  "license": "MIT",
  "author": "Vadim Gnezdyshkin <vadim.gnezdyshkin@bl.uk>",
  "homepage": "https://github.com/vadim-libraryon/strapi-plugin-icon-picker#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vadim-libraryon/strapi-plugin-icon-picker.git"
  },
  "bugs": {
    "url": "https://github.com/vadim-libraryon/strapi-plugin-icon-picker/issues"
  },
  "keywords": [
    "strapi",
    "strapi-plugin",
    "custom-field",
    "icons",
    "svg",
    "react"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.25.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-intl": "^7.1.11",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.25.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/typescript-utils": "^5.25.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-intl": "^7.1.11",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "strapi": {
    "kind": "plugin",
    "name": "icon-picker",
    "displayName": "Icon Picker",
    "description": "Pick icons from local React/SVG files with auto-discovery and grid/search options."
  }
}
